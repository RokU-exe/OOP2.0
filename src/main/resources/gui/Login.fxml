<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="373.0" prefWidth="497.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.LoginController">
    <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="157.0" prefWidth="497.0" style="-fx-font-size: 18pt;" text="Insurance Claims Management System" />
    <HBox alignment="CENTER" spacing="10">
        <Label alignment="TOP_LEFT" prefHeight="17.0" prefWidth="75.0" text="Email:" />
        <TextField fx:id="emailField" alignment="TOP_LEFT" prefHeight="25.0" prefWidth="225.0" />
      <VBox.margin>
         <Insets bottom="10.0" />
      </VBox.margin>
    </HBox>
    <HBox alignment="CENTER" spacing="10">
        <Label prefHeight="17.0" prefWidth="75.0" text="Password:" />
        <PasswordField fx:id="passwordField" prefHeight="25.0" prefWidth="225.0" />
      <VBox.margin>
         <Insets />
      </VBox.margin>
    </HBox>
    <Button fx:id="loginButton" contentDisplay="CENTER" onAction="#handleLogin" prefHeight="25.0" prefWidth="134.0" style="-fx-alignment: CENTER;" text="Login" textAlignment="CENTER">
      <VBox.margin>
         <Insets top="20.0" />
      </VBox.margin></Button>
</VBox>
